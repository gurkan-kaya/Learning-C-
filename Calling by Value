using System;

namespace ConsoleApp8
{
    class Program
    {
        
         static void degistir(int a, int b)
            {
                int temp = b;
                b = a;
                a = temp;
            }

        static void Main(string[] args)
        {
            int a = 10;
            int b = 20;

            degistir(a,b);
            Console.WriteLine("{0}-{1}",a,b);


        }
    }
}
//Output is: 10-20
